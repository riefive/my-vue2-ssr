{
  "name": "vue2-example-ssr",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build:spa": "vue-cli-service build",
    "build:ssr": "export SSR=true && vue-cli-service build",
    "build": "yarn build:spa && yarn build:ssr",
    "clear": "rimraf .appview && rimraf bundle && rimraf dist",
    "run:spa": "dotenv -e .env.test.spa node index",
    "run:ssr": "dotenv -e .env.test.ssr node index",
    "start:dev": "dotenv -e .env node index",
    "start:production": "node build && cd .appview && yarn install --production=true && export NODE_ENV=production && node index",
    "test:unit": "vue-cli-service test:unit"
  },
  "dependencies": {
    "core-js": "^3.6.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "vue": "^2.6.11",
    "vue-router": "^3.2.0",
    "vue-server-renderer": "^2.6.12",
    "vuex": "^3.4.0",
    "vuex-persist": "^3.1.3"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "~4.5.0",
    "@vue/cli-plugin-router": "~4.5.0",
    "@vue/cli-plugin-unit-jest": "~4.5.0",
    "@vue/cli-plugin-vuex": "~4.5.0",
    "@vue/cli-service": "~4.5.0",
    "@vue/test-utils": "^1.0.3",
    "cpx": "^1.5.0",
    "dotenv-cli": "^4.0.0",
    "node-sass": "^4.12.0",
    "rimraf": "^3.0.2",
    "sass-loader": "^8.0.2",
    "vue-template-compiler": "^2.6.11",
    "webpack-node-externals": "^2.5.2"
  },
  "author": {
    "name": "riefive",
    "email": "rie.five@gmail.com"
  },
  "license": "ISC"
}
